Handlebars.registerHelper('ifNotEmpty',function(a,b){return $.isEmptyObject(a)?b.inverse(this):b.fn(this)}),Handlebars.registerHelper('debug',function(a){console.log(JSON.stringify(a))}),Handlebars.registerHelper('compare',function(a,operator,b,options){function isString(a){return'string'==typeof a||a instanceof String}function stringWrap(a){return isString(a)?'"'+a+'"':a}if(4>arguments.length)throw new Error('Helper {{compare}} expects 4 arguments.');if(0>$.inArray(operator,['==','===','!=','!==','<','>','<=','>=']))throw new Error('helper {{compare}}: invalid operator: `'+operator+'`');let expr=''+stringWrap(a)+operator+stringWrap(b),retVal=eval(expr)||!1;return retVal?options.fn(this):options.inverse(this)}),Handlebars.registerHelper('getTypeHashLink',function(a,b){if(b.data){var c=app.util.getHashFromTypeString(a);b.data.typeHashLink=c}}),Handlebars.registerHelper('setLang',function(a){return a.data&&(data=Handlebars.createFrame(a.data)),data.currentLang=app.state.getLang(),a.fn(this,{data:data})}),Handlebars.registerHelper('ifAddOnContent',function(a,b){if(a=Handlebars.escapeExpression(a),b.data&&b.data.currentLang){var c=this[a],d=c?c[b.data.currentLang]:null;if(d)return data=Handlebars.createFrame(b.data),data.addOnContent=d,b.fn(this,{data:data})}return b.inverse(this)});
