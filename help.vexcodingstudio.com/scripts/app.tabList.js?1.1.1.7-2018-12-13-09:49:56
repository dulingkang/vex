"use strict";app&&(app.tabList=function(){var a={init:function(){console.log("init tablist"),a.bind(),a.adjustScrollbarHeight()},updateCM:function(){if(0==$("div.selected div.CodeMirror").get().length){var b=$("div.selected textarea.snippet").get();b.forEach(function(a){CodeMirror.fromTextArea(a,{lineNumbers:!0,matchBrackets:!0,editorType:"code",mode:"text/x-c++src",cursorBlinkRate:-1,readOnly:!0})})}a.adjustScrollbarHeight()},adjustScrollbarHeight:function(){let a=$("#content"),b=a.height();console.log("adjusting height "+b);let c=$("#content .refPage"),d=c.height();d+100>b?c.css("padding-bottom",Math.max(100,b-d)):c.css("padding-bottom","")},bind:function(){$(".tabSelector").on("click",function(a){$(a.target).find("button").trigger("click")}),$(".tabSelector pre").on("click",function(a){$(a.target).parent().trigger("click")}),$(".tabSelector button").on("click",function(b){b.stopPropagation();var c=b.target.id,d=c.replace("button","overload");return $(".tabSelector.selected").removeClass("selected"),$("#"+c).parent().addClass("selected"),$(".tabContent.selected").removeClass("selected"),$("#"+d).addClass("selected"),$(".tabSelector button").text("+"),$("#"+c).text("-"),a.updateCM(),b.preventDefault(),!1}),$("a.typeLink").on("click",function(a){a.stopPropagation();var b=a.target;return"A"!=b.tagName.toUpperCase()&&(b=$(b).closest("a")[0]),app.state.selectMember(b.id,b.text)})}};return{init:a.init,bind:a.bind}}());
