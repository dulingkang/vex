"use strict";app&&(app.treeView=function(){var a={bind:function(){$("li.treeViewLeaf").on("click",function(a){$(a.target).find("a").trigger("click")}),$(".treeViewLeaf a").on("click",function(a){a.stopPropagation();var b=a.target;return"A"!=b.tagName.toUpperCase()&&(b=$(b).closest("a")[0]),app.state.selectMember(b.id,b.text)}),$("li.treeViewItem").on("click",function(b){b.stopPropagation();var c=$(b.target);"SPAN"==c.prop("tagName")&&(c=c.parent()),"DIV"==c.prop("tagName")&&(c=c.parent()),a.toggleExpanded(c.children("ul.treeViewGroup").first())}),$(".hamburger").on("click",function(a){a.stopPropagation(),app.state.toggleHamburger()}),a.toggleExpanded($("ul.treeViewGroup")),a.toggleExpanded($("ul.treeViewRoot")),$(document).on("navChanged",function(){let b=app.state.getMember();if(b){var c=$("a#"+app.state.getLang()+"\\/"+b.replace(/\//g,"\\/"));a.expandItemContainers(c.parent()),a.setSelectedMember(c)}}),$(window).on("resize",function(){a.adjustScrollbarHeight()}),$("#treeView").animate({scrollTop:0})},toggleExpanded:function(b){b.toggle();let c=b.parent().children("div").first().children("span").first();1==c.length&&(b.is(":hidden")?c.text("+"):c.text("-")),a.adjustScrollbarHeight()},adjustScrollbarHeight:function(){let a=$("#treeView"),b=a.height(),c=$(".treeViewRoot").height();c+150>b?a.css("padding-bottom",Math.max(150,b-c)):a.css("padding-bottom","")},expandItemContainers:function(b){var c=[];$.each(b.parents(),function(a,b){var d=$(b);d.hasClass("treeViewGroup")&&d.is(":hidden")&&c.push(d)}),c=c.reverse(),c.forEach(function(b){a.toggleExpanded(b)})},scrollItemIntoView:function(a){var b=$("#treeView")[0].scrollTop+a.offset().top-200;$("#treeView").animate({scrollTop:b}),$("#main").hide().show(0)},setSelectedMember:function(b){$("li.treeViewLeaf").removeClass("selected"),0<b.length&&(b.parent().addClass("selected"),a.scrollItemIntoView(b))}};return{bind:a.bind}}());
